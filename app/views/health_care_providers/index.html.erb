<p id="notice"><%= notice %></p>

<h1>Health Care Providers</h1>

<table>
  <thead>
    <tr>
      <th>Hcp</th>
      <th>Company name</th>
      <th>Address</th>
      <th>City</th>
      <th>State</th>
      <th>Telephone</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @health_care_providers.each do |health_care_provider| %>
      <tr>
        <td><%= health_care_provider.hcp_id %></td>
        <td><%= health_care_provider.company_name %></td>
        <td><%= health_care_provider.address %></td>
        <td><%= health_care_provider.city %></td>
        <td><%= health_care_provider.state %></td>
        <td><%= health_care_provider.telephone %></td>
        <td><%= link_to 'Show', health_care_provider %></td>
        <td><%= link_to 'Edit', edit_health_care_provider_path(health_care_provider) %></td>
        <td><%= link_to 'Destroy', health_care_provider, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Health Care Provider', new_health_care_provider_path %>
